{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "node_terraform_ecs_app",
    "containerDefinitions": [
        {
            "name": "node_terraform_ecs_app",
            "image": "708980201424.dkr.ecr.us-west-2.amazonaws.com/newput/node-terraform:latest",
            "essential": true,
            "cpu": "1024",
            "memory":"2048",
            "portMappings": [
              {
                "containerPort": 80,
                "hostPort"     : 80
              }
            ]
  
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "2048",
    "cpu": "1024",
    "executionRoleArn": "arn:aws:iam::708980201424:role/ecsTaskExecutionRole"
  }
